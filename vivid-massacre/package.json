{
    "name": "vivid-massacre",
    "displayName": "Vivid Massacre",
    "description": "A theme specially created for moments such as 'I will murder the person who wrote it'",
    "publisher": "vverum",
    "author": {
        "name": "Dawid Sobala"
    },
	"license": "MIT",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.56.0"
    },
    "categories": [
        "Themes"
    ],
    "contributes": {
        "themes": [
            {
                "label": "Vivid Massacre",
                "uiTheme": "vs-dark",
                "path": "./themes/Vivid Massacre-color-theme.json"
            }
        ],
        "languages": [
            {
                "id": "csharp",
                "aliases": [
                    "c#",
                    "csharp"
                ],
                "extensions": [
                    ".cs"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "csharp",
                "scopeName": "extendedSource.cs",
                "path": "./syntaxes/csharp.tmLanguage.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/vverum/VS_Code_Themes.git"
    }
}